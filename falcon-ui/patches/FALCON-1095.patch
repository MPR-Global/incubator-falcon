+
+                    var source = layout.node(u);
+                    var target = layout.node(v);
+
+                    var p = points.length === 0 ? source : points[points.length - 1];
+
+                    var r = CIRCLE_RADIUS;
+                    var sx = p.x, sy = p.y, tx = target.x, ty = target.y;
+                    var l = Math.sqrt((tx - sx) * (tx - sx) + (ty - sy) * (ty - sy));
