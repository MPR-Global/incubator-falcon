+
+        }, true);
+
+        scope.selectedRows = [];
+        scope.mirrorTag = "_falcon_mirroring_type";
+
+        scope.checkedRow = function (name) {
+          var isInArray = false;
+          scope.selectedRows.forEach(function(item) {
+            if (name === item.name) {
+              isInArray = true;
+            }
+          });
