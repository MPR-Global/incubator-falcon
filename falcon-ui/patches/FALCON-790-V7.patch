+    background-color: #fff;
+    padding: 0.5625rem 0.875rem;
+    -webkit-box-shadow: 0 0.3125rem 0.625rem rgba(0,0,0,0.2);
+    -moz-box-shadow: 0 0.3125rem 0.625rem rgba(0,0,0,0.2);
